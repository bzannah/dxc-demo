{
  "name": "@uniformdev/context",
  "version": "18.12.0",
  "description": "Uniform Context core package",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./cli": {
      "types": "./dist/cli/cli.d.ts",
      "import": "./dist/cli/cli.mjs",
      "require": "./dist/cli/cli.js"
    },
    "./api": {
      "types": "./dist/api/api.d.ts",
      "import": "./dist/api/api.mjs",
      "require": "./dist/api/api.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli/cli.d.ts"
      ],
      "api": [
        "./dist/api/api.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s update-openapi build:ts",
    "build:ts": "tsup",
    "dev": "run-s update-openapi dev:ts",
    "dev:ts": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "update-openapi": "tsx ./scripts/update-openapi.cts",
    "benchmark:build": "tsup src/storage/__benchmarks__/storage.benchmark.ts",
    "benchmark:run": "node ./dist/storage.benchmark.js"
  },
  "devDependencies": {
    "@types/js-cookie": "3.0.2",
    "@types/yargs": "17.0.22",
    "@uniformdev/cli": "18.12.0",
    "benny": "3.7.1",
    "yargs": "17.6.2"
  },
  "dependencies": {
    "dequal": "^2.0.2",
    "js-cookie": "3.0.1",
    "mitt": "^3.0.0",
    "p-limit": "^3.1.0",
    "rfdc": "^1.3.0"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "80d99ef272482848ca9a2508937fd19cc0f23508"
}
