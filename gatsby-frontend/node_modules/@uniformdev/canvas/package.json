{
  "name": "@uniformdev/canvas",
  "version": "18.12.0",
  "description": "Common functionality and types for Uniform Canvas",
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./dist/index.mjs",
        "default": "./dist/index.esm.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./cli": {
      "types": "./dist/cli/cli.d.ts",
      "import": "./dist/cli/cli.mjs",
      "require": "./dist/cli/cli.js"
    }
  },
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.ts"
      ],
      "cli": [
        "./dist/cli/cli.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s update-openapi build:ts",
    "build:ts": "tsup",
    "dev": "run-s update-openapi dev:ts",
    "dev:ts": "tsup --watch",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "update-openapi": "tsx ./scripts/update-openapi.cts"
  },
  "devDependencies": {
    "@types/retry": "0.12.1",
    "@types/yargs": "17.0.22",
    "@uniformdev/cli": "18.12.0",
    "p-limit": "4.0.0",
    "p-retry": "5.1.2",
    "p-throttle": "5.0.0",
    "pusher-js": "8.0.1",
    "yargs": "17.6.2"
  },
  "dependencies": {
    "@uniformdev/context": "18.12.0"
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "80d99ef272482848ca9a2508937fd19cc0f23508"
}
